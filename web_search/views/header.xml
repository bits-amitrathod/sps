<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" inherit_id="web.assets_frontend" name="Website Assets">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/web_search/static/src/css/search.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/web_search/static/src/js/lib.js"></script>
            <script type="text/javascript" src="/web_search/static/src/js/custom_search.js"></script>
        </xpath>
    </template>
</odoo>